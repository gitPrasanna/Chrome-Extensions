{
	"manifest_version": 2,
	"name": "mini access",
	"description": "Access your application with any where.",
	"version": "1.0",
	"permissions": [
		"storage"
	],
	"icons": { 
		// "16": "img/icon16.png",
		// "48": "img/icon48.png",
		// "128": "img/icon128.png" 
		"16": "img/tpro348.png",
		"48": "img/tpro348.png",
		"128": "img/tpro348.png" 
	},
	"browser_action": {
		"default_icon": {                    
			// "19": "img/icon48.png",          
			// "38": "img/icon48.png"          
			"19": "img/tpro348.png",          
			"38": "img/tpro348.png"          
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	]
}