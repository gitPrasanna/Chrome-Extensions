{
	"manifest_version": 2,
	"name": "TPro mini access",
	"description": "Access TPro easily for get catch .",
	"version": "1.0",
	"permissions": [
		"storage"
	],
	"icons": {
		"16": "img/tpro16.png",
		"48": "img/tpro48.png",
		"128": "img/tpro128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/tpro128.png",
			"38": "img/tpro128.png"
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	]
}